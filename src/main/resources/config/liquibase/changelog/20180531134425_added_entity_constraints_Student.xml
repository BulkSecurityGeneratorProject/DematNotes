<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20180531134425-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="student"
                                 constraintName="fk_student_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="school_year_id"
                                 baseTableName="student"
                                 constraintName="fk_student_school_year_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school_year"/>

        <addForeignKeyConstraint baseColumnNames="classroom_id"
                                 baseTableName="student"
                                 constraintName="fk_student_classroom_id"
                                 referencedColumnNames="id"
                                 referencedTableName="classroom"/>

        <addForeignKeyConstraint baseColumnNames="school_id"
                                 baseTableName="student"
                                 constraintName="fk_student_school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school"/>

    </changeSet>
</databaseChangeLog>

<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity InscriptionModule.
    -->
    <changeSet id="20180531134426-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="module_id"
                                 baseTableName="inscription_module"
                                 constraintName="fk_inscription_module_module_id"
                                 referencedColumnNames="id"
                                 referencedTableName="module"/>

        <addForeignKeyConstraint baseColumnNames="student_id"
                                 baseTableName="inscription_module"
                                 constraintName="fk_inscription_module_student_id"
                                 referencedColumnNames="id"
                                 referencedTableName="student"/>

    </changeSet>
</databaseChangeLog>
